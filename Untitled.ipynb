    "import json\n",
    "import pandas\n",
    "from collections import Counter\n",
    "\n",
    "with open(\"conversations.jsonl\") as f:\n",
    "    lengths = Counter(len(json.loads(line)['conversation']) for line in f)\n",
    "    \n",
    "data = pandas.DataFrame.from_dict(lengths, orient='index')\n",
    "print(data.dtypes)\n",
    "data.plot()\n",
    "\n"
